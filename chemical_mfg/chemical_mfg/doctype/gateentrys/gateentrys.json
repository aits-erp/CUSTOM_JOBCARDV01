{
  "actions": [],
  "allow_rename": 1,
  "doctype": "DocType",
  "engine": "InnoDB",
  "is_submittable": 1,

  "module": "Chemical Mfg",
  "name": "GateEntrys",

  "field_order": [
    "section_break_5szl",
    "gate_entry_type",
    "status",
    "posting_date",
    "vehicle_no",
    "transporter_name",
    "supplier",

    "section_weight",
    "gross_weight",
    "tare_weight",
    "net_weight",

    "section_items",
    "gate_entry_items",

    "section_amend",
    "amended_from",
    "remarks"
  ],

  "fields": [
    {
      "fieldname": "section_break_5szl",
      "fieldtype": "Section Break",
      "label": "Gate Entry Details"
    },

    {
      "fieldname": "gate_entry_type",
      "fieldtype": "Select",
      "label": "Gate Entry Type",
      "options": "Inward\nOutward",
      "reqd": 1
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "options": "Draft\nVehicle In\nUnloading\nCompleted\nCancelled",
      "default": "Draft",
      "read_only": 1
    },
    {
      "fieldname": "posting_date",
      "fieldtype": "Date",
      "label": "Posting Date",
      "default": "Today",
      "reqd": 1
    },
    {
      "fieldname": "vehicle_no",
      "fieldtype": "Data",
      "label": "Vehicle No",
      "reqd": 1
    },
    {
      "fieldname": "transporter_name",
      "fieldtype": "Data",
      "label": "Transporter Name"
    },
    {
      "fieldname": "supplier",
      "fieldtype": "Link",
      "label": "Supplier",
      "options": "Supplier",
      "depends_on": "eval:doc.gate_entry_type=='Inward'"
    },

    {
      "fieldname": "section_weight",
      "fieldtype": "Section Break",
      "label": "Weighment Details"
    },
    {
      "fieldname": "gross_weight",
      "fieldtype": "Float",
      "label": "Gross Weight"
    },
    {
      "fieldname": "tare_weight",
      "fieldtype": "Float",
      "label": "Tare Weight"
    },
    {
      "fieldname": "net_weight",
      "fieldtype": "Float",
      "label": "Net Weight",
      "read_only": 1
    },

    {
      "fieldname": "section_items",
      "fieldtype": "Section Break",
      "label": "Items"
    },
    {
      "fieldname": "gate_entry_items",
      "fieldtype": "Table",
      "label": "Gate Entry Items",
      "options": "Gate Entry Item",
      "reqd": 1
    },

    {
      "fieldname": "section_amend",
      "fieldtype": "Section Break",
      "label": "Amendment"
    },
    {
      "fieldname": "amended_from",
      "fieldtype": "Link",
      "label": "Amended From",
      "options": "GateEntrys",
      "no_copy": 1,
      "read_only": 1,
      "print_hide": 1,
      "search_index": 1
    },
    {
      "fieldname": "remarks",
      "fieldtype": "Small Text",
      "label": "Remarks"
    }
  ],

  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "submit": 1,
      "print": 1,
      "email": 1,
      "export": 1,
      "report": 1,
      "share": 1
    }
  ]
}
